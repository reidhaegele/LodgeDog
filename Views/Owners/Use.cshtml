                <label asp-for="Password" class="control-label"></label>
                <input asp-for="Password" class="form-control" readonly />
                <span asp-validation-for="Password" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Guestpasses" class="control-label"></label>
                <input asp-for="Guestpasses" class="form-control" type="number" min="0" />
                <span asp-validation-for="Guestpasses" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Reservationpasses" class="control-label"></label>
                <input asp-for="Reservationpasses" class="form-control" type="number" min="0" />
