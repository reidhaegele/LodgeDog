                <span asp-validation-for="Fullservice" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Signupdate" class="control-label"></label>
                <input asp-for="Signupdate" class="form-control" />
                <span asp-validation-for="Signupdate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Firstname" class="control-label"></label>
                <input asp-for="Firstname" class="form-control" maxlength="255" required/>
                <span asp-validation-for="Firstname" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Lastname" class="control-label"></label>
                <input asp-for="Lastname" class="form-control" maxlength="255" required/>
