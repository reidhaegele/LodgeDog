            </div>
            <div class="form-group">
                <label asp-for="Primarycontact" class="control-label"></label>
                <input asp-for="Primarycontact" class="form-control" maxlength="255" />
                <span asp-validation-for="Primarycontact" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Phonenumber" class="control-label"></label>
                <input asp-for="Phonenumber" class="form-control" type="tel" maxlength="255" placeholder="123-4567-8901" pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}" />
                <span asp-validation-for="Phonenumber" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Alternatephonenumber" class="control-label"></label>
                <input asp-for="Alternatephonenumber" class="form-control" type="tel" value="0" maxlength="255" placeholder="123-4567-8901" pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}" />
                <span asp-validation-for="Alternatephonenumber" class="text-danger"></span>
